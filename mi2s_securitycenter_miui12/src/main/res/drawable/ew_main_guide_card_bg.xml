<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/ew_main_guide_card_bg_press_color"/>
            <stroke android:width="@dimen/ew_main_guide_card_bg_stoke_width" android:color="@color/ew_main_guide_card_stoke_color"/>
            <corners android:radius="@dimen/ew_main_guide_card_bg_corner"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/ew_main_guide_card_bg_color"/>
            <stroke android:width="@dimen/ew_main_guide_card_bg_stoke_width" android:color="@color/ew_main_guide_card_stoke_color"/>
            <corners android:radius="@dimen/ew_main_guide_card_bg_corner"/>
        </shape>
    </item>
</selector>
